{
	"editor.formatOnSave": true,
	"workbench.editor.labelFormat": "medium",
	"workbench.editor.untitled.labelFormat": "content",
	"svelte.plugin.svelte.format.config.printWidth": 120,
	"svelte.plugin.svelte.format.config.singleQuote": true,
	"svelte.plugin.svelte.format.config.svelteStrictMode": true,
	"javascript.format.semicolons": "insert",
	"typescript.format.semicolons": "insert",
	"editor.tabSize": 2,
	"editor.fontFamily": "Fira Code Retina",
	"editor.fontLigatures": "'calt', 'ss01', 'ss02', 'ss03', 'ss04', 'ss05', 'ss06', 'zero', 'onum'",
	"workbench.editor.enablePreview": false
}
